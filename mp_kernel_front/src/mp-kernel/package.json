{
  "name": "sql-kernel",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20.16.0"
  },
  "exports": {
    "./App": "./src/pages/App",
    "./GlobalHeader": "./src/components/GlobalHeader",
    "./i18n": "./src/i18n/configs",
    "./TopPage": "./src/pages/TopPage",
    "./Init": "./src/components/Init"
  },
  "scripts": {
    "dev": " webpack serve --mode=development",
    "prod": "yarn-run webpack serve --mode=production",
    "dev-build": "tsc && webpack --mode=development",
    "prod-build": "tsc && webpack --mode=production",
    "dev-build-watch": "webpack -w --mode=development",
    "analyze": "ANALYZE=true yarn prod-build",
    "prettier": "prettier --write . --ignore-path .gitignore --log-level warn",
    "eslint": "eslint '**/*.{js,ts,jsx,tsx}' --rulesdir node_modules/@sql/configs/src/eslint/rules --ignore-path .gitignore",
    "prepare": "cd ../.. && husky src/sql-kernel/.husky",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 5500",
    "build-storybook": "storybook build",
    "test": "jest --config ./jest.config.cjs",
    "coverage": "jest --config ./jest.config.cjs --coverage",
    "generate": "docker compose --env-file .env -f node_modules/@mp/openapi-generator/docker/docker-compose.yml run -u $(id -u):$(id -g) --build --rm generator",
    "postinstall": "create-env",
    "textlint": "unset $(env | awk -F= '$1 ~ /^npm_/ {print $1}') && textlint * --rulesdir node_modules/@sql/configs/src/textlint/rules --ignore-path .gitignore",
    "stylelint": "stylelint **/*.scss"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "^7.22.23",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/open-sans": "^5.2.7",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.12.1",
    "@sql/configs": "file:.yalc/@sql/configs",
    "@sql/sql-libs": "file:.yalc/@sql/sql-libs",
    "@storybook/testing-library": "^0.1.0",
    "@tiptap/extension-image": "^3.7.2",
    "@tiptap/extension-link": "^3.7.2",
    "@tiptap/react": "^3.7.2",
    "@tiptap/starter-kit": "^3.7.2",
    "axios": "^1.9.0",
    "date-fns": "^2.30.0",
    "i18next": "^23.12.3",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.26.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@sql/sql-libs": "file:.yalc/@sql/sql-libs",
    "@storybook/react": "^7.6.20",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/typescript-estree": "^7.18.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "storybook": "^7.6.20",
    "stylelint": "^15.11.0",
    "textlint": "^13.4.1",
    "typescript": "-5.5.4",
    "webpack": "^5.94.0",
    "yalc": "^1.0.0-pre.53"
  },
  "uniqueDependencies": [],
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
